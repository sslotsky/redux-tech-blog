{
  "presets": ['es2015', 'react', 'stage-0'],
  "plugins": [
    ["module-resolver", {
      "root": ["./"],
      "alias": {
        "SERVER": "./app/server/",
        "MODELS": "./app/server/models/",
        "SERVICES": "./app/server/services/",

        "CLIENT": "./app/client/",
        "MODULES": "./app/client/modules",
        "SHARED": "./app/client/shared",
        "POSTS": "./app/client/modules/posts"
      }
    }]
  ]
}

